{
	"name": "ydj-h5-client",
	"version": "0.0.0-beta1",
	"private": true,
	"description": "this is ydj miniprogram client",
	"templateInfo": {
		"name": "redux",
		"typescript": true,
		"css": "less"
	},
	"scripts": {
		"deploy:preview": "node scripts/deploy type=preview",
		"deploy:upload": "node scripts/deploy type=upload",
		"build:weapp": "taro build --type weapp",
		"build:swan": "taro build --type swan",
		"build:alipay": "taro build --type alipay",
		"build:tt": "taro build --type tt",
		"build:h5": "taro build --type h5",
		"build:rn": "taro build --type rn",
		"build:qq": "taro build --type qq",
		"build:jd": "taro build --type jd",
		"build:quickapp": "taro build --type quickapp",
		"dev:weapp": "npm run build:weapp -- --watch",
		"dev:watch:weapp": "NODE_ENV=production taro build --type weapp --watch",
		"dev:swan": "npm run build:swan -- --watch",
		"dev:alipay": "npm run build:alipay -- --watch",
		"dev:tt": "npm run build:tt -- --watch",
		"dev:h5": "npm run build:h5 -- --watch",
		"dev:rn": "npm run build:rn -- --watch",
		"dev:qq": "npm run build:qq -- --watch",
		"dev:jd": "npm run build:jd -- --watch",
		"dev:quickapp": "npm run build:quickapp -- --watch",
		"lint:js": "eslint --cache --ext .ts,.tsx --format=pretty ./src",
		"prepare": "husky install"
	},
	"lint-staged": {
		"**/*.less": "stylelint --syntax less",
		"*.ts?(x)": [
			"eslint --cache --format=pretty"
		],
		"*.{ts,tsx,js,jsx,less,md,json}": [
			"prettier --write"
		]
	},
	"browserslist": [
		"last 3 versions",
		"Android >= 4.1",
		"ios >= 8"
	],
	"author": "",
	"license": "MIT",
	"dependencies": {
		"@babel/runtime": "^7.7.7",
		"@tarojs/components": "3.3.0",
		"@tarojs/react": "3.3.0",
		"@tarojs/runtime": "3.3.0",
		"@tarojs/taro": "3.3.0",
		"ahooks": "^2.10.6",
		"classnames": "^2.3.1",
		"querystring": "^0.2.1",
		"react": "^17.0.0",
		"react-dom": "^17.0.0",
		"react-redux": "^7.2.0",
		"redux": "^4.0.0",
		"redux-logger": "^3.0.6",
		"redux-thunk": "^2.3.0",
		"taro-ui": "^3.0.0-alpha.10"
	},
	"devDependencies": {
		"@babel/core": "^7.8.0",
		"@tarojs/cli": "3.3.0",
		"@tarojs/mini-runner": "3.3.0",
		"@tarojs/webpack-runner": "3.3.0",
		"@types/react": "^17.0.2",
		"@types/webpack-env": "^1.13.6",
		"@typescript-eslint/eslint-plugin": "^4.15.1",
		"@typescript-eslint/parser": "^4.15.1",
		"@umijs/fabric": "^2.6.2",
		"babel-preset-taro": "3.3.0",
		"dotenv": "^10.0.0",
		"dotenv-expand": "^5.1.0",
		"eslint": "^6.8.0",
		"eslint-config-taro": "3.3.0",
		"eslint-plugin-import": "^2.12.0",
		"eslint-plugin-react": "^7.8.2",
		"eslint-plugin-react-hooks": "^4.2.0",
		"husky": "^6.0.0",
		"lint-staged": "^11.0.0",
		"miniprogram-ci": "^1.4.13",
		"prettier": "^2.3.1",
		"stylelint": "^13.13.1",
		"typescript": "^4.1.0"
	}
}
